@model ExpenseAppMvc.Models.ExpenseFormHistory
    ViewBag.Title = "ExpenseFormHistoryUpdate";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>ExpenseFormHistoryUpdate</h2>

@using (Html.BeginForm("ExpenseFormInformation", "ExpenseForm", FormMethod.Post))
{
    @Html.HiddenFor(x => x.Id)

    @Html.LabelFor(x => x.ExpenseFormId)
    @Html.TextBoxFor(x => x.ExpenseFormId, new { @class = "form-control" })
    <br />

    @Html.LabelFor(x => x.ExpenseFormStatusId)

    @Html.DropDownListFor(x => x.ExpenseFormStatusId, (List<SelectListItem>)ViewBag.newExpenseStatusValue, new { @class = "form-control" })
    @*@Html.TextBoxFor(x => x.ExpenseFormStatusId, new { @class = "form-control" })*@
    <br />
    @Html.LabelFor(x => x.UserId)
    @*@Html.TextBoxFor(x => x.UserId, new { @class = "form-control" })*@
    @Html.DropDownListFor(x => x.UserId, (List<SelectListItem>)ViewBag.newuser, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.ExpenseFormCurrencyId)
    @*@Html.TextBoxFor(x => x.ExpenseFormCurrencyId, new { @class = "form-control" })*@
    @Html.DropDownListFor(x => x.ExpenseFormCurrencyId, (List<SelectListItem>)ViewBag.newvalue, new { @class = "form-control" })
    <br />

    @Html.LabelFor(x => x.ExpenseFormDescription)
    @Html.TextBoxFor(x => x.ExpenseFormDescription, new { @class = "form-control" })
    <br />

    @Html.LabelFor(x => x.ExpenseFormDescription)
    @Html.TextBoxFor(x => x.ExpenseFormDescription, new { @class = "form-control" })
    <br />

    @Html.LabelFor(x => x.CreateDate)
    @Html.TextBoxFor(x => x.CreateDate, new { @class = "form-control" })
    <br />

    @Html.LabelFor(x => x.IsExpenseFormDeleted)
    @Html.TextBoxFor(x => x.IsExpenseFormDeleted, new { @class = "form-control" })
    <br />

    <button class="btn btn-warning" id="updatebtn">UPDATE</button>

    <script>
        $('#btn1').click(function () {
            swal("Expense Forms History Updated!");
        });
    </script>

}

