
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Expense App</title>

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Project Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript">
        addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
               function hideURLbar(){ window.scrollTo(0,1); } </script>

    <link href="//fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i&amp;subset=cyrillic,cyrillic-ext,greek,greek-ext,latin-ext,vietnamese" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Raleway:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&amp;subset=latin-ext" rel="stylesheet">

    <link href="~/weblogin/css/style.css" type="text/css" rel="stylesheet" media="all">

    <link rel="stylesheet" href="~/weblogin/css/font-awesome.css"> <!-- Font-Awesome-Icons-CSS -->

    <link href="~/weblogin/css/popup-box.css" rel="stylesheet" type="text/css" media="all" /> <!-- popup box css -->

</head>
<body>
    <section class="register">
        <div class="header">
            <div class="logo">
                <a href="#"> Login</a>
            </div>

            <div class="clear"></div>
        </div>

        <div class="register-full">
            <div class="register-left">
                <div class="register">

                </div>
            </div>
            <div class="register-right">
                <div class="register-in">
                    <a class="book popup-with-zoom-anim button-isi zoomIn animated" data-wow-delay=".5s" href="#small-dialog2">Register »</a>
                </div>
                <div class="register-in middle">
                    <a class="book popup-with-zoom-anim button-isi zoomIn animated" data-wow-delay=".5s" href="#small-dialog">Employee »</a>
                </div>
                <div class="register-in">
                    <a class="book popup-with-zoom-anim button-isi zoomIn animated" data-wow-delay=".5s" href="#small-dialog1">Account »</a>
                </div>
                <div class="register-in middle">
                    <a class="book popup-with-zoom-anim button-isi zoomIn animated" data-wow-delay=".5s" href="#small-dialog3">Manager »</a>
                </div>
                <div class="register-in">
                    <a class="book popup-with-zoom-anim button-isi zoomIn animated" data-wow-delay=".5s" href="#small-dialog4">Admin »</a>
                </div>
                <div class="clear"> </div>
            </div>
            <div class="clear"> </div>
        </div>
        <!-- copyright -->
        <!-- //copyright -->
    </section>
    <!-- //section -->
    <!-- login form popup-->
    <div class="pop-up">
        <div id="small-dialog" class="mfp-hide book-form">
            <div class="login-form login-form-left">
                <div class="agile-row">
                    <h3>Employee Form</h3>
                    <span class="fa fa-lock"></span>
                    <div class="clear"></div>
                    <div class="login-agileits-top">
                        @using (Html.BeginForm("Login1", "Login", FormMethod.Post))
                        {
                            <input type="text" class="Username" name="username" Placeholder="Username" required="" />
                            <input type="password" class="Password" name="Password" Placeholder="Password" required="" />
                            <input type="submit" value="Login">
                        }
                    </div>
                    <div class="login-agileits-bottom">
                    </div>
                </div>
            </div>

        </div>
    </div>
    <div class="pop-up">
        <div id="small-dialog1" class="mfp-hide book-form">
            <div class="login-form login-form-left">
                <div class="agile-row">
                    <h3>Accountant Form</h3>
                    <div class="clear"></div>
                    <div class="login-agileits-top">
                        @using (Html.BeginForm("AccountantLogin", "Login", FormMethod.Post))
                        {
                            <input type="text" class="Username" name="username" Placeholder="Username" required="" />
                            <input type="password" class="Password" name="Password" Placeholder="Password" required="" />
                            <input type="submit" value="Login">
                        }
                    </div>
                </div>
            </div>

        </div>
    </div>
    <div class="pop-up">
        <div id="small-dialog4" class="mfp-hide book-form">
            <div class="login-form login-form-left">
                <div class="agile-row">
                    <h3>Admin Form</h3>
                    <span class="fa fa-lock"></span>
                    <div class="clear"></div>
                    <div class="login-agileits-top">
                        @using (Html.BeginForm("AdminLogin", "Login", FormMethod.Post))
                        {
                            <input type="text" class="Username" name="username" Placeholder="Username" required="" />
                            <input type="password" class="Password" name="Password" Placeholder="Password" required="" />
                            <input type="submit" value="Login">
                        }
                    </div>
                    <div class="login-agileits-bottom">
                    </div>
                </div>
            </div>

        </div>
    </div>


    <div class="pop-up">
        <div id="small-dialog3" class="mfp-hide book-form">
            <div class="login-form login-form-left">
                <div class="agile-row">
                    <h3>Manager Form</h3>
                    <span class="fa fa-lock"></span>
                    <div class="clear"></div>
                    <div class="login-agileits-top">
                        @using (Html.BeginForm("ManagerLogin", "Login", FormMethod.Post))
                        {
                            <input type="text" class="Username" name="username" Placeholder="Username" required="" />
                            <input type="password" class="Password" name="Password" Placeholder="Password" required="" />
                            <input type="submit" value="Login">
                        }
                    </div>
                    <div class="login-agileits-bottom">
                    </div>
                </div>
            </div>

        </div>
    </div>






    <!-- //subscribe form popup-->
    <!-- register form popup-->
    <div class="pop-up">
        <div id="small-dialog2" class="mfp-hide book-form">

            @Html.Action("Partial1", "Login");
        </div>
    </div>
    <!-- //register form popup-->
    <!-- js -->
    <script type="text/javascript" src="~/weblogin/js/jquery-2.1.4.min.js"></script>
    <!-- //js -->
    <!--popup-js-->
    <script src="~/weblogin/js/jquery.magnific-popup.js" type="text/javascript"></script>
    <script>
        $(document).ready(function () {
            $('.popup-with-zoom-anim').magnificPopup({
                type: 'inline',
                fixedContentPos: false,
                fixedBgPos: true,
                overflowY: 'auto',
                closeBtnInside: true,
                preloader: false,
                midClick: true,
                removalDelay: 300,
                mainClass: 'my-mfp-zoom-in'
            });

        });
    </script>
    <!--//popup-js-->

</body>
<!-- //body ends -->
</html>